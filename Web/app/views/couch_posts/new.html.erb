<h1> Crear Publicación de Couch</h1>

<%= form_for :couch_post, url: couch_posts_path do |c| %>
	<p>
		<%= c.label :Título %> <br>
		<%= c.text_field :title %>
	</p>

	<p>
		<%= c.label :Descripción %> <br>
		<%= c.text_area :description %>
	</p>

<p>
		<%= c.label :Dueño %> <br>
		<%= c.text_area :username %>
	</p>


	<p>

		<%= c.submit %>
	</p>
	<%end%>